node{
    def app
    stage('scm'){
        git branch: 'main', url: 'https://github.com/kranthi5121/dockerhub-private.git'
    }
    stage('build the image'){
        app=docker.build("kranthi5121/privaterepo",".")
    }
    stage('push image'){
        docker.withRegistry('https://registry.hub.docker.com','Dockerhub')
        {
            app.push("latest")
        }
    }
}
